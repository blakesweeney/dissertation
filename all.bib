isbn = {1476-928X (Electronic)$\backslash$n1476-9271 (Linking)},
issn = {14769271},
journal = {Computational Biology and Chemistry},
keywords = {Prediction,RNA tertiary structure,Three-way junctions},
pages = {1--5},
pmid = {22326420},
title = {{Automated prediction of three-way junction topological families in RNA secondary structures}},
volume = {37},
year = {2012}
}
