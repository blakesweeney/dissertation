journal = {Nucleic Acids Research},
keywords = {Base Pairing,Models, Molecular,Nucleic Acid Conformation,RNA, Ribosomal,RNA, Ribosomal: chemistry,Sequence Alignment,Sequence Analysis, RNA,Sequence Analysis, RNA: methods,Sequence Homology, Nucleic Acid},
