keywords = {16S,16S: chemistry,23S,23S: chemistry,5S,5S: chemistry,Archaeal,Archaeal: chemistry,Bacterial,Bacterial: chemistry,Base Pairing,Base Sequence,Conserved Sequence,Crystallography,Evolution,Guanine,Guanine: chemistry,Models,Molecular,Molecular Sequence Data,Nucleic Acid Conformation,Phosphates,Phosphates: chemistry,RNA,Ribosomal,Ribosomal: chemistry,Ribosomes,Ribosomes: chemistry,Sequence Analysis,Uracil,Uracil: chemistry,X-Ray},
month = {jan},
