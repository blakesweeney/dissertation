issn = {09074449},
journal = {Acta Crystallographica Section D: Biological Crystallography},
keywords = {difference density,electron density,model accuracy,model precision,real-space R,real-space correlation coefficient,real-space difference density Z score,real-space observed density Z score,refinement and validation,structure validation Model building},
number = {4},
