journal = {RNA (New York, N.Y.)},
keywords = {Algorithms,Base Pairing,Base Sequence,Computer Simulation,Models,Molecular,Nucleic Acid Conformation,RNA,RNA: chemistry,RNA: genetics,RNA: metabolism},
number = {3},
pages = {410--6},
pmid = {18230758},
title = {{From knotted to nested RNA structures: a variety of computational methods for pseudoknot removal.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2248259{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {14},
