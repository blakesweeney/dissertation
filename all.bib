@article{Cate1996,
abstract = {The crystal structure of a group I intron domain reveals an unexpected motif that mediates both intra- and intermolecular interactions. At three separate locations in the 160-nucleotide domain, adjacent adenosines in the sequence lie side-by-side and form a pseudo-base pair within a helix. This adenosine platform opens the minor groove for base stacking or base pairing with nucleotides from a noncontiguous RNA strand. The platform motif has a distinctive chemical modification signature that may enable its detection in other structured RNAs. The ability of this motif to facilitate higher order folding provides one explanation for the abundance of adenosine residues in internal loops of many RNAs.},
author = {Cate, J H and Gooding, a R and Podell, E and Zhou, K and Golden, B L and Szewczak, a a and Kundrot, C E and Cech, T R and Doudna, J a},
