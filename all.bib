title = {{RNA tertiary structure mediation by adenosine platforms.}},
volume = {273},
year = {1996}
}
@article{Szewczak1993a,
abstract = {The sarcin/ricin loop is a highly conserved sequence found in the RNA of all large ribosomal subunits. The cytotoxins alpha-sarcin and ricin both inactivate ribosomes by cleaving a single bond in that loop. Once it has been attacked, ribosomes no longer interact with elongation factors properly, and translation stops. We have determined the conformation of the sarcin/ricin loop by multinuclear NMR spectroscopy using E73, a 29-nucleotide RNA that has the sarcin/ricin loop sequence and that is sensitive to both toxins in vitro. The sarcin/ricin loop has a compact structure that contains several purine.purine base pairs, a GAGA tetraloop, and a bulged guanosine adjacent to a reverse Hoogsteen A.U pair. It is stabilized by an unusual set of cross-strand base-stacking interactions and imino proton to phosphate oxygen hydrogen bonds. In addition to having interesting structural features, this model explains many of the biochemical observations made about the loop's structure and its reactivity with cytotoxins, and it sheds light on the loop's interactions with elongation factors.},
author = {Szewczak, A A and Moore, P B and Chang, Y L and Wool, I G},
doi = {10.1073/pnas.90.20.9581},
file = {:Users/bsweene/Documents/Papers/Proceedings of the National Academy of Sciences of the United States of America/1993/The conformation of the sarcinricin loop from 28S ribosomal RNA.pdf:pdf},
isbn = {0027-8424 (Print)$\backslash$r0027-8424 (Linking)},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Animals,Base Sequence,Endoribonucleases,Fungal Proteins,Fungal Proteins: toxicity,Hydrogen Bonding,Magnetic Resonance Spectroscopy,Models, Molecular,Molecular Sequence Data,Nucleic Acid Conformation,Oligonucleotides,Oligonucleotides: chemistry,RNA, Ribosomal, 28S,RNA, Ribosomal, 28S: chemistry,RNA, Ribosomal, 28S: ultrastructure,Rats,Ricin,Ricin: toxicity,Thermodynamics},
number = {20},
pages = {9581--5},
pmid = {8415744},
title = {{The conformation of the sarcin/ricin loop from 28S ribosomal RNA.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=47613{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {90},
year = {1993}
}
@article{Gutell1990,
abstract = {The data base of prokaryotic small subunit ribosomal RNAs alone now numbers more than 400 sequences, while that for the large subunit rRNAs numbers more than 70 when eukaryotic, mitochondrial, and plastid sequences are also included. Comparisons among these rRNA sequences reveal a number of positions that covary in composition, suggestive of higher order structural elements; 5 such structures are reported for the small subunit rRNA and 15 for the large subunit rRNA. While some of these are properly (small) secondary structural elements, the majority would have to be classified as more complex "tertiary" interactions, which in some cases bring together diverse areas in the secondary structural diagram. A number of the covariances are not of the canonical type, indicating non-Watson-Crick interactions.},
author = {Gutell, R R and Woese, C R},
doi = {10.1073/pnas.87.2.663},
isbn = {0027-8424 (Print)},
